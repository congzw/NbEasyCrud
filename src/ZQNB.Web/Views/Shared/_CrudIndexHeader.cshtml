@model ModelMetadata
@*@Html.Raw(Newtonsoft.Json.JsonConvert.SerializeObject(Model.Properties.First()))
<hr/>*@
<tr>
    @foreach (var prop in Model.Properties.Where(p => p.ShowForEdit))
    {
        if (!"HiddenInput".Equals(prop.TemplateHint, StringComparison.OrdinalIgnoreCase))
        {
            <th>@prop.DisplayName</th>
        }
    }
    <th>操作</th>
</tr>